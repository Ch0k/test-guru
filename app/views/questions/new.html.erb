

<form accept-charset="UTF-8" action="/tests/<%= @test %>/questions" method="post">
  <label for="question-id">Question</label>
  <input type="text" name="question[body]" id="question-id"/>
  <input type="hidden" name="question[test_id]" value="<%= @test %>"/> 
  <input type="hidden" value="<%= form_authenticity_token %>" name="authenticity_token"/>
  <input type="submit" value="create">
</form>

