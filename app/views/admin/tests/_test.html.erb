<tr>
  <td class="align-middle">
  <%= content_tag :span, test.title, class: "test-title", data: { test_id: test.id } %>
  <%= render 'form_inline', test: test == @test? @test : test %>
  
  </td>
  <td class="align-middle"><%= test.level %>
  <td class="align-middle"><%= test.questions.count %>
  <td class="align-middle"><%= link_to t('.show'), admin_test_path(test) %></td>
  <td class="align-middle"><%= link_to t('.edit'), 
                                                  edit_admin_test_path(test),
                                                  class: "form_inline_link",
                                                  data: {test_id: test.id}
                                                  %></td>
  <td class="align-middle"><%= link_to t('.destroy'), admin_test_path(test), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  <td class="align-middle"><%= button_to t('.start'), start_test_path(test) %>
</tr>
