<%= question_header %>
<%= form_with model: @question, local: true do |form| %>
<%= render 'shared/errors', resource: @question %>
<p>
  <%= form.label :body %>
  <%= form.text_field :body, id: :test_title, class: "form-control" %>
</p>
<p><%= form.submit %>
<% end %>
<%= link_to 'Back', admin_test_path(@question.test) %>
