<h2>Test: <%= @test.title %><br>
<h2>Level: <%= @test.level %> </h2>
<%= link_to 'Edit', edit_test_path(@test) %> |
<%= link_to 'Back', tests_path %> <br>
<%= link_to 'New Question', new_test_question_path(@test_id) %>
<h3>List questions:</h3>
<table> 
  <tr>
    <th>body
  </tr>
<% @questions.each do |question| %>
  <tr>
    <td><%= question.body %>
    <td><%= link_to 'Edit', edit_question_path(question) %></td>
    <td><%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
  
<% end %>